<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.toxicpointer.clubmanager.gui.contoller.TableController">
    <BorderPane fx:id="contentPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
                AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0"
    >
        <top>
            <Label text="Tabelle" BorderPane.alignment="TOP_CENTER">
                <BorderPane.margin>
                    <Insets bottom="10.0" top="10.0"/>
                </BorderPane.margin>
                <font>
                    <Font name="Arial Bold" size="22.0"/>
                </font>
            </Label>
        </top>
        <center>
            <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                <columns>
                    <TableColumn fx:id="placeColumn" minWidth="0.0" prefWidth="54.0" resizable="false" text="Platz"/>
                    <TableColumn fx:id="clubColumn" minWidth="0.0" resizable="false" text="Verein"/>
                    <TableColumn fx:id="goalColumn" prefWidth="92.0" resizable="false" text="Tore"/>
                    <TableColumn fx:id="concededColumn" prefWidth="126.0" resizable="false" text="Gegentore"/>
                    <TableColumn fx:id="pointsColumn" prefWidth="245.0" resizable="false" text="Punkte"/>
                </columns>
                <BorderPane.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                </BorderPane.margin>
            </TableView>
        </center>
    </BorderPane>
</AnchorPane>